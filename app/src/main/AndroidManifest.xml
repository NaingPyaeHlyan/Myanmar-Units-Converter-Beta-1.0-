<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fairway.com.mm.myunit">

    <application
        android:allowBackup="true"
        android:icon="@drawable/convert"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LengthActivity" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MassActivity" android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>